<p id="notice"><%= notice %></p>

<h1>Listing Thanas</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @thanas.each do |thana| %>
      <tr>
        <td><%= thana.name %></td>
        <td><%= link_to 'Show', thana %></td>
        <td><%= link_to 'Edit', edit_thana_path(thana) %></td>
        <td><%= link_to 'Destroy', thana, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Thana', new_thana_path %>
